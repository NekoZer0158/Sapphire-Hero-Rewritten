"res://addons/sapphire_hero_mods/Scripts/": "yellow",
"res://addons/sapphire_hero_mods/Sprites/": "blue",
"res://z_Documentation/": "gray"
}

[global_group]

Moving_enemy_body=""
Flying_enemy_body=""
Doors=""

[gui]

theme/custom_font="res://Fonts/PublicPixel.ttf"

