config_version=5

[application]

config/name="Sapphire Hero Rewritten"
config/version="2.4"
config/tags=PackedStringArray("готово")
run/main_scene="res://Scenes/UI/Main_menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://Audio/default_bus_layout.tres"

[autoload]

GlobalSapphire="*res://Scripts/Singletons/GlobalSapphire.gd"
GlobalEnum="*res://Scripts/Singletons/GlobalEnum.gd"
GlobalMods="*res://addons/sapphire_hero_mods/Scripts/Singletons/GlobalMods.gd"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PackedStringArray("res://addons/sapphire_hero_mods/plugin.cfg")

[file_customization]

folder_colors={
"res://Audio/": "purple",
"res://Fonts/": "gray",
"res://Resources/": "orange",
"res://Scenes/": "red",
"res://Scripts/": "yellow",
"res://Sprites/": "blue",
"res://Translations/": "green",
"res://addons/": "pink",
